{
  "version": 2,
  "builds": [{ "src": "./index.js", "use": "@vercel/node" }],
  "rewrites": [{ "source": "/(.*)", "destination": "./index.js" }],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://fiverr-clone-ruddy.vercel.app"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./index.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "UPDATE"]
    }
  ]
}
